{
    "name": "hyperf/barrier",
    "description": "A barrier library for Hyperf",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "barrier"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "issues": "https://github.com/hyperf/hyperf/issues",
        "source": "https://github.com/hyperf/hyperf",
        "docs": "https://hyperf.wiki",
        "pull-request": "https://github.com/hyperf/hyperf/pulls"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Barrier\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "hyperf/di": "~3.1.0",
        "hyperf/support": "~3.1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Barrier\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Barrier\\ConfigProvider"
        }
    }
}
